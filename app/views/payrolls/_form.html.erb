<div class="card">    
  <div class="card-content">
    <%= simple_form_for(@store_payroll) do |f| %>
      <%= f.error_notification %>
      <div class="row">
        <div class="form-inputs">
          <%= f.input :start_date, label_html: { class: 'active' } %>
          <%= f.input :end_date, label_html: { class: 'active' }, include_blank: true %>
      </div>
    </div>
  </div>
</div>
  <div class="row">
    <div class="col s12">
      <%= render :partial => "store_payroll_fields", locals: {f: f} %>
    </div>
  </div>
  <div class="row">
    <div class="form-actions col s7">
      <%= f.button :submit %>
    </div>
  </div>
<% end %>
